package Euro2016;

public class Bang {
	private int idBang;
	private String tenBang;
	public int getIdBang() {
		return idBang;
	}
	public void setIdBang(int idBang) {
		this.idBang = idBang;
	}
	public String getTenBang() {
		return tenBang;
	}
	public void setTenBang(String tenBang) {
		this.tenBang = tenBang;
	}
	Bang b1 = new Bang();
	Bang b2 = new Bang();
	Bang b3 = new Bang();
	Bang b4 = new Bang();
	b1.setIdBang("1");
	b2.setIdBang("2");
	b3.setIdBang("3");
	b4.setIdBang("4");
}
