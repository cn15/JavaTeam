package Euro2016;

public class HatGiong {
	private int idHatgiong;
	private String tenHatgiong;
	public int getIdHatgiong() {
		return idHatgiong;
	}
	public void setIdHatgiong(int idHatgiong) {
		this.idHatgiong = idHatgiong;
	}
	public String getTenHatgiong() {
		return tenHatgiong;
	}
	public void setTenHatgiong(String tenHatgiong) {
		this.tenHatgiong = tenHatgiong;
	}
}
