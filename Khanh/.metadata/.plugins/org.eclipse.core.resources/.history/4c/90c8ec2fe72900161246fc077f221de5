package Euro2016;

public class Team {
	private int idTeam;
	private int idBang;
	private String tenTeam;
	private int idHatgiong;
	public int getIdTeam() {
		return idTeam;
	}
	public void setIdTeam(int idTeam) {
		this.idTeam = idTeam;
	}
	public int getIdBang() {
		return idBang;
	}
	public void setIdBang(int idBang) {
		this.idBang = idBang;
	}
	public String getTenTeam() {
		return tenTeam;
	}
	public void setTenTeam(String tenTeam) {
		this.tenTeam = tenTeam;
	}
	public int getIdHatgiong() {
		return idHatgiong;
	}
	public void setIdHatgiong(int idHatgiong) {
		this.idHatgiong = idHatgiong;
	}
	
}
