package Euro2016;
public class Bang {
	private int idBang;
	private String tenBang;
	public int getIdBang() {
		return idBang;
	}
	public void setIdBang(int idBang) {
		this.idBang = idBang;
	}
	public String getTenBang() {
		return tenBang;
	}
	public void setTenBang(String tenBang) {
		this.tenBang = tenBang;
	}
}