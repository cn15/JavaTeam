public class Nhap {

	public static void main(String[] args) {
		try{
			BufferedReader iK=new BufferedReader(new InputStreamReader (System.in));
			Human a = new Students();
			System.out.print("\nNhap ten: ");
			a.setName()=iK.readLine();
		}
		catch (Exception e){
